
{
  "name": "ReDownload",
  "manifest_version": 2,
  "version": "1.0",
  "icons": {
    "16": "Re-Download.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "downloads",
    "storage"
  ],
  "background": {
    "scripts": [
      "memorize.js",
      "storage"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.4.1.min.js", "re_download.js"]
    }
  ]
}